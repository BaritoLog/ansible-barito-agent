{% for source in tdagent_sources %}
<source>
{% for key in source|sort %}
{% if source[key] is mapping %}
  <{{ key }}>
{% for child_key in source[key]|sort %}
    {{ child_key }} {{ source[key][child_key] }}
{% endfor %}
  </{{ key }}>
{% else %}
  {{ key }} {{ source[key] }}
{% endif %}
{% endfor %}
</source>
{% endfor %}